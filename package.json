{
  "name": "ol-deep-research-orchestrator",
  "version": "0.1.0",
  "description": "Deep Research Orchestrator service for GPT-OSS workflows",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run build:ui && npm run build:server",
    "build:server": "tsc -p tsconfig.json",
    "build:ui": "npm --prefix ui run build",
    "dev:ui": "npm --prefix ui run dev",
    "start:ui": "node dist/uiServer.js",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"{src,prompts}/**/*.{ts,md}\"",
    "migrate": "ts-node src/migrate.ts"
  },
  "keywords": [
    "deep-research",
    "orchestrator"
  ],
  "author": "Oremus Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@fastify/static": "^8.3.0",
    "@qdrant/js-client-rest": "1.11.0",
    "fastify": "^5.6.2",
    "fastify-sensible": "^3.1.2",
    "minio": "^8.0.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "prom-client": "^15.1.3",
    "undici": "^7.16.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.1",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.7.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
